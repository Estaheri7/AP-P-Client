<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<DialogPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.appclient.Controllers.UpdateSkillController">
   <content>
      <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label text="Update skills">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="skill1TextField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="500.0" promptText="Skill ">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="skill2TextField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="500.0" promptText="Skill">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="skill3TextField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="500.0" promptText="Skill">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="skill4TextField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="500.0" promptText="Skill">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="skill5TextField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="500.0" promptText="Skill">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButton" prefWidth="100.0" text="Save">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </content>
</DialogPane>
